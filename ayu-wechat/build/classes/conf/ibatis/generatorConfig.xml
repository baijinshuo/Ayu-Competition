<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!-- 数据库驱动 -->
	<classPathEntry location="E:\java_about\mysql-connector-java-5.1.18-bin.jar" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库链接URL，用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://101.200.124.68:3306/ayu_db" userId="root" password="ayu_admin">
		</jdbcConnection>
		<!-- <javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver> -->
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.template.framework.domain"
			targetProject="ayu-wx">
			<property name="enableSubPackages" value="true" />
			<!-- <property name="trimStrings" value="true" /> -->
		</javaModelGenerator>
		<!-- 生成映射文件的包名和位置 -->
		<sqlMapGenerator targetPackage="com.template.framework.mapper"
			targetProject="ayu-wx">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.template.framework.dao" targetProject="ayu-wx">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- 要生成哪些表 -->
		<table tableName="wx_user" domainObjectName="WxUser"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>
	</context>
</generatorConfiguration>
